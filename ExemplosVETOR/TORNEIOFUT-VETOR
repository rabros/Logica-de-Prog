algoritmo "EXERCICIO 1 AULA 14"
var
   times: vetor[1..3] de caractere
   i, j, aux: inteiro
inicio
   Para i <-1 ate 3 faca
      Escreva("Digite um time: ")
      Leia(times[i])
   FimPara
   Para i <- 1 ate 3 faca
      Para j <- 1 ate 3 faca
         Se (times[i] <> times[j]) entao
            EscrevaL(times[i]," X ", times[j])
         FimSe
      FimPara
   FimPara
fimalgoritmo