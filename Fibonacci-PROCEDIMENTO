algoritmo "FIBONACCI"
var
   C, T1, T2, T3: INTEIRO
Procedimento ProximoFibonacci(var A, B: INTEIRO)
var T: inteiro
inicio
    T <- A + B
    Escreva (T)
    A <- B
    B <- T
Fimprocedimento
inicio
      T1 <- 0
      Escreva (T1)
      T2 <- 1
      Escreva (T2)
      Para C := 3 ate 15 faca
         ProximoFibonacci(T1, T2)
      FimPara
fimalgoritmo